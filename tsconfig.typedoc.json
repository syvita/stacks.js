{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES6"
  },
  "typedocOptions": {
    "readme": "overview.md",
    "excludePrivate": true,
    "excludeProtected": true,
    "external-modulemap": ".*\/packages\/([\\w\\-_]+)\/",
    "gitRevision": "master",
    "listInvalidSymbolLinks": true
  }
}
